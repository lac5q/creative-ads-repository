# Google Ads API Configuration
# You need to obtain these credentials from Google Ads API Console
# Follow the setup guide: https://developers.google.com/google-ads/api/docs/first-call/overview

# Required: Your Google Ads Developer Token
# Get this from: https://ads.google.com/aw/apicenter
GOOGLE_ADS_DEVELOPER_TOKEN=your_developer_token_here

# Required: OAuth2 Client Credentials
# Get these from Google Cloud Console > APIs & Services > Credentials
GOOGLE_ADS_CLIENT_ID=your_client_id_here.apps.googleusercontent.com
GOOGLE_ADS_CLIENT_SECRET=your_client_secret_here

# Required: Refresh Token (obtained during OAuth flow)
# This is generated when you first authenticate
GOOGLE_ADS_REFRESH_TOKEN=your_refresh_token_here

# Required: Customer ID (your Google Ads account ID)
# Format: 123-456-7890 (without dashes in actual value: 1234567890)
GOOGLE_ADS_CUSTOMER_ID=1234567890

# Optional: Login Customer ID (for MCC accounts)
# Only needed if you're using a Manager Account (MCC)
GOOGLE_ADS_LOGIN_CUSTOMER_ID=

# Application Environment
APP_ENV=dev

# Optional: Enable specific features
ENABLE_BUDGET_MANAGEMENT=true
ENABLE_KEYWORD_MANAGEMENT=true
ENABLE_AD_GROUP_MANAGEMENT=true
ENABLE_AUTOMATED_INSIGHTS=true

# Optional: Cache settings
CACHE_TTL_SECONDS=300
ENABLE_CACHING=true

# Optional: Rate limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=60 